ainer\r\n      if (banner.querySelector('iframe')) {\r\n        banner.classList.add('show');\r\n        clearInterval(interval);\r\n      }\r\n      if (checkCount >= maxChecks) {\r\n        clearInterval(interval);\r\n        \/\/ Optional: you can hide banner if no ad found after timeout\r\n        banner.style.display = 'none';\r\n      }\r\n    }, 500);\r\n  }\r\n\r\n  window.addEventListener('load', waitForAd);\r\n<\/script>\r\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t<div class=\"has_eae_slider elementor-element elementor-element-5385114 e-con-full e-flex wpr-particle-no wpr-jarallax-no wpr-parallax-no wpr-sticky-section-no e-con e-child\" data-id=\"5385114\" data-element_type=\"container\">\n\t\t\t\t<div class=\"elementor-element elementor-element-68b6ee9 exad-sticky-section-no exad-glass-effect-no jltma-glass-effect-no elementor-widget elementor-widget-html\" data-id=\"68b6ee9\" data-element_type=\"widget\" data-widget_type=\"html.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t<!-- Orbitron Font -->\r\n<link href=\"https:\/\/fonts.googleapis.com\/css2?family=Orbitron:wght@600&display=swap\" rel=\"stylesheet\">\r\n\r\n<!-- DarkNet Radar Banner -->\r\n<div id=\"darknet-radar-widget\">\r\n  <h3>\ud83d\udce1 DarkNet News Radar \r\n    <button id=\"darknet-close-btn\" onclick=\"toggleDarknetWidget()\">\u2715<\/button>\r\n  <\/h3>\r\n  <ul id=\"darknet-news-list\">\r\n    <li><a href=\"#\">Loading latest headlines...<\/a><\/li>\r\n  <\/ul>\r\n<\/div>\r\n\r\n<!-- DarkNet Radar CSS -->\r\n<style>\r\n  #darknet-radar-widget {\r\n    position: fixed;\r\n    bottom: 10px;\r\n    left: 10px;\r\n    width: 280px;\r\n    height: 90px;\r\n    background: rgba(18, 18, 18, 0.92);\r\n    border: 1px solid rgba(0, 255, 204, 0.3);\r\n    border-radius: 8px;\r\n    overflow: hidden;\r\n    color: #66ccc9;\r\n    font-family: monospace, monospace;\r\n    font-size: 0.8rem;\r\n    padding: 10px 10px 4px 10px;\r\n    box-sh