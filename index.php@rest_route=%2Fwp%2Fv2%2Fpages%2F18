al;\r\n  let isWidgetVisible = true;\r\n\r\n  async function loadDarknetNews() {\r\n    try {\r\n      const response = await fetch('\/.netlify\/functions\/news'); \/\/ Fixed path\r\n      if (!response.ok) throw new Error(`HTTP error! status: ${response.status}`);\r\n      headlines = await response.json();\r\n      console.log('Loaded headlines:', headlines);\r\n      updateNewsList();\r\n      startAutoScroll();\r\n    } catch (error) {\r\n      console.error('Failed to load news:', error);\r\n      const list = document.getElementById('darknet-news-list');\r\n      list.innerHTML = '<li><a href=\"#\">Error loading news: ' + error.message + '<\/a><\/li>';\r\n    }\r\n  }\r\n\r\n  function updateNewsList() {\r\n    const list = document.getElementById('darknet-news-list');\r\n    list.innerHTML = '';\r\n    headlines.forEach((item, index) => {\r\n      const li = document.createElement('li');\r\n      li.className = index === 0 ? 'active' : ''; \/\/ First item active\r\n      const a = document.createElement('a');\r\n      a.href = item.link || '#';\r\n      a.target = '_blank';\r\n      a.rel = 'noopener noreferrer';\r\n      a.textContent = item.title || 'No title';\r\n      li.appendChild(a);\r\n      list.appendChild(li);\r\n    });\r\n    if (!headlines.length) {\r\n      list.innerHTML = '<li><a href=\"#\">No headlines available.<\/a><\/li>';\r\n    }\r\n  }\r\n\r\n  function startAutoScroll() {\r\n    if (scrollInterval) clearInterval(scrollInterval);\r\n    const listItems = document.querySelectorAll('#darknet-news-list li');\r\n    if (listItems.length <= 1) return; \/\/ No scroll if 0 or 1 item\r\n\r\n    scrollInterval = setInterval(() => {\r\n      const currentItem = document.querySelector('#darknet-news-list li.active');\r\n      if (currentItem) currentItem.classList.remove('active');\r\n      currentIndex = (currentIndex + 1) % listItems.length;\r\n      listItems[currentIndex].classList.add('active');\r\n    }, 8000); \/\/ 8 seconds for slow scroll\r\n  }\r\n\r\n  function stopAutoScroll() {\r\n    if (scrollInterval) clearInterval(scrollInterval);\r\n  }\r\n\r\n  function toggleDarknetWidget() {\r\n    const widget = document.getElementById('darknet-radar-widget');\r\n    if (isWidgetVisible) {\r\n      widget.style.display = 'none';\r\n      stopAutoScroll();\r\n      isWidgetVisible = false;\r\n    } else {\r\n      widget.style.display = 'block';\r\n      startAutoScroll();\r\n      isWidgetVisible = true;\r\n    }\r\n  }\r\n\r\n  \/\/ Load + refresh every 30 minutes\r\n  document.addEventListener('DOMContentLoaded', loadDarknetNews);\r\n  setInterval(loadDarknetNews, 60 * 1000);\r\n<\/script>\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t\t\t<\/div>\n\t\t<div class=\"has_eae_slider elementor-element elementor-element-a9429d3 e-con-full e-flex wpr-particle-no wpr-jarallax-no wpr-parallax-no wpr-sticky-section-no e-con e-child\" data-id=\"a9429d3\" data-element_type=\"container\">\n\t\t\t\t<div class=\"elementor-element elementor-element-6590471 exad-sticky-section-no exad-glass-effect-no jltma-glass-effect-no elementor-widget elementor-widget-html\" data-id=\"6590471\" data-element_type=\"widget\" data-widget_type=\"html.default\">\n\t\t\t\t<div class=\"elementor-widget-container\">\n\t\t\t\t\t<!-- Floating IP Tracker Panel -->\r\n<div id=\"ip-tracker-panel\">\r\n  <div id=\"ip-toggle\" onclick=\"togglePanel()\">\r\n    <span class=\"icon\">\ud83d\udef0\ufe0f<\/span>\r\n  <\/div>\r\n  <div id=\"ip-content\">\r\n    <h3 class=\"ip-heading\"><span class=\"icon\">\ud83d\udef0\ufe0f<\/span> Your IP Address Info<\/h3>\r\n    <div id=\"ip-result\">Loading...<\/div>\r\n  <\/div>\r\n<\/div>\r\n\r\n<style>\r\n#ip-tracker-panel {\r\n  position: fixed;\r\n  top: 70px;\r\n  left: 1rem;\r\n  z-index: 9999;\r\n  font-family: 'Share Tech Mono', monospace;\r\n}\r\n#ip-toggle {\r\n  background: linear-gradient(145deg, #0c0c0c, #121212);\r\n  border: 1px solid #00ffcc66;\r\n  border-radius: 50%;\r\n  width: 50px;\r\n  height: 50px